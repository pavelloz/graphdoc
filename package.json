{
  "name": "pos-graphdocs",
  "version": "2.6.4",
  "description": "Static page generator for documenting GraphQL Schema",
  "main": "bin/graphdoc",
  "bin": {
    "graphdoc": "./bin/graphdoc.js"
  },
  "scripts": {
    "watch": "tsc -w -p .",
    "compile": "tsc -p .",
    "declaration": "tsc -d -p tsconfig.json",
    "doc.github": "node bin/graphdoc.js -c example.github.json -f -n html",
    "doc.pos": "node bin/graphdoc.js -s http://127.0.0.1:8080/schema.graphql -f -n html",
    "test": "jest"
  },
  "license": "MIT",
  "jest": {
    "testRegex": "\\.(test|spec)\\.js$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.8",
    "@types/fs-extra": "8.0.1",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.1.1",
    "@types/marked": "0.7.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.5.3",
    "@types/request": "^2.48.4",
    "@types/striptags": "^3.1.1",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "jest": "^26.0.1",
    "pkg-dir": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@2fd/command": "^1.1.2",
    "bluebird": "^3.7.2",
    "deepmerge": "^4.2.2",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "graphql": "^15.0.0",
    "marked": "^1.0.0",
    "mustache": "^4.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "slug": "^2.1.1",
    "striptags": "^3.1.1",
    "word-wrap": "^1.2.3"
  },
  "graphdoc": {
    "schemaFile": "http://localhost:8080/schema.graphql",
    "output": "./gh-pages/pos",
    "baseUrl": "./"
  }
}

{
  "name": "@2fd/graphdoc",
  "version": "2.4.0",
  "description": "Static page generator for documenting GraphQL Schema",
  "main": "bin/graphdoc",
  "bin": {
    "graphdoc": "./bin/graphdoc.js"
  },
  "scripts": {
    "sass": "node-sass -w --output-style compressed template/slds/styles/graphdoc.scss > template/slds/styles/graphdoc.css",
    "compile": "tsc -p .",
    "declaration": "tsc -d -p tsconfig.json",
    "doc.github": "node bin/graphdoc.js -c example.github.json -f -v",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2fd/graphdoc.git"
  },
  "author": "Fede Ramirez <i@2fd.me>",
  "license": "MIT",
  "jest": {
    "testRegex": "\\.(test|spec)\\.js$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@salesforce-ux/design-system": "2.2.1",
    "@types/bluebird": "^3.0.37",
    "@types/chai": "^3.4.34",
    "@types/fs-extra": "0.0.32",
    "@types/glob": "^5.0.30",
    "@types/graphql": "^0.8.6",
    "@types/jest": "^18.1.1",
    "@types/marked": "0.0.27",
    "@types/mocha": "^2.2.32",
    "@types/mustache": "^0.8.28",
    "@types/node": "^6.0.32",
    "@types/request": "^2.47.0",
    "@types/striptags": "^3.1.1",
    "chai": "^3.5.0",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "jest": "^25.1.0",
    "node-sass": "^4.12.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@2fd/command": "^1.1.2",
    "bluebird": "^3.5.5",
    "deepmerge": "^2.0.1",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.0",
    "graphql": "^0.12.0",
    "marked": "^0.7.0",
    "mustache": "^2.2.1",
    "request": "^2.88.0",
    "slug": "^0.9.1",
    "striptags": "^3.1.1",
    "word-wrap": "^1.2.1"
  },
  "graphdoc": {
    "schemaFile": "./test/github.json",
    "output": "./gh-pages/github",
    "baseUrl": "./"
  }
}

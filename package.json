{
  "name": "graphdocs",
  "version": "2.4.0",
  "description": "Static page generator for documenting GraphQL Schema",
  "main": "bin/graphdoc",
  "bin": {
    "graphdoc": "./bin/graphdoc.js"
  },
  "scripts": {
    "watch": "npx webpack-cli -w",
    "build": "NODE_ENV=production npx webpack-cli",
    "compile": "tsc -p .",
    "declaration": "tsc -d -p tsconfig.json",
    "doc.github": "node bin/graphdoc.js -c example.github.json -f -n html",
    "doc.pos": "node bin/graphdoc.js -s http://127.0.0.1:8080/schema.graphql -f -n html",
    "test": "jest"
  },
  "license": "MIT",
  "jest": {
    "testRegex": "\\.(test|spec)\\.js$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/chai": "^3.4.34",
    "@types/fs-extra": "0.0.32",
    "@types/glob": "^5.0.30",
    "@types/graphql": "^0.8.6",
    "@types/jest": "^18.1.1",
    "@types/marked": "0.0.27",
    "@types/mocha": "^2.2.32",
    "@types/mustache": "^0.8.28",
    "@types/node": "^6.0.32",
    "@types/request": "^2.47.0",
    "@types/striptags": "^3.1.1",
    "autoprefixer": "^9.7.4",
    "chai": "^4.2.0",
    "css-loader": "^3.4.2",
    "css-mqpacker": "^7.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pkg-dir": "^4.2.0",
    "postcss": "^7.0.26",
    "postcss-csso": "^4.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "request-promise": "^4.2.5",
    "terser-webpack-plugin": "^2.3.2",
    "typescript": "^3.5.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@2fd/command": "^1.1.2",
    "bluebird": "^3.7.2",
    "deepmerge": "^4.2.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "graphql": "^14.5.8",
    "marked": "^0.8.0",
    "mustache": "^4.0.0",
    "request": "^2.88.0",
    "slug": "^2.1.0",
    "striptags": "^3.1.1",
    "word-wrap": "^1.2.3"
  },
  "graphdoc": {
    "schemaFile": "http://localhost:8080/schema.graphql",
    "output": "./gh-pages/pos",
    "baseUrl": "./"
  }
}
